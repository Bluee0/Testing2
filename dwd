local function QKYWPUT_fake_script() -- PizzaDLC.Rainbow.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.Rainbow)

	high2 = false
	local Frame = script.Parent.Circle
	script.Parent.MouseButton1Down:connect(function()
	
		if high2 == true then 
			high2 = false 
		else
			high2 = true
		end
		if high2 == true then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(180, 5, 255)
			Frame:TweenPosition(UDim2.new(0.566, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1)
		end
		if high2 == false then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
			Frame:TweenPosition(UDim2.new(0.036, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1) 
		end
	
		if high2 == true then
			repeat
				wait(0)
				ESP.Color = script.Parent.Parent.Parent.RainbowC.BackgroundColor3
			until  high2 == false
			script.Parent.Parent.Tr3.Visible = true
			script.Parent.Parent.SliderR.Visible = true
			script.Parent.Parent.Trc3.Visible = true
		end
	
		if high2 == false then 
			script.Parent.Parent.Tr3.Visible = false
			script.Parent.Parent.SliderR.Visible = false
			script.Parent.Parent.Trc3.Visible = false
		end
	end)
	
end
coroutine.wrap(QKYWPUT_fake_script)()
local function BMTNC_fake_script() -- PizzaDLC.RainbowC.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.RainbowC)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
end
coroutine.wrap(BMTNC_fake_script)()
