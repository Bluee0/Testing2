local function FYJQ_fake_script() -- PizzaDLC.Fov.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.Fov)

	fovu = false
	local Frame = script.Parent.Circle
	script.Parent.MouseButton1Down:connect(function()
	
		if fovu == true then 
			fovu = false 
		else
			fovu = true
		end
		if fovu == true then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(180, 5, 255)
			Frame:TweenPosition(UDim2.new(0.566, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1)
	
		end
		if fovu == false then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
			Frame:TweenPosition(UDim2.new(0.036, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1) 
	
		end
	
		if fovu == true then
			_G.CF = true
			script.Parent.Parent.Frame.Visible = true
			script.Parent.Parent.Slider4.Visible = true
			script.Parent.Parent.Frame2.Visible = true
		end
	
		if fovu == false then 
			_G.CF = false
			script.Parent.Parent.Frame.Visible = false
			script.Parent.Parent.Slider4.Visible = false
			script.Parent.Parent.Frame2.Visible = false
		end
	end)
	
end
coroutine.wrap(FYJQ_fake_script)()
local function BFFB_fake_script() -- PizzaDLC.Slider4.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.Slider4)

	function newSlider(title, callback, min, max, startPoint)
		local dragging = false;
		local sliderFrame = script.Parent.Parent.Slider4
		local slidingFrame = script.Parent.Slide4
		local circle = slidingFrame.Frame
		local titleLabel = script.Parent.stuff4
	
		local function slide(input)
			local pos = UDim2.new(math.clamp((input.Position.X - sliderFrame.AbsolutePosition.X) / sliderFrame.AbsoluteSize.X, 0, 1), 0, 1, 0);
			slidingFrame:TweenSize(pos, Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.0, true);
			local value = math.floor(((pos.X.Scale * max) / max) * (max - min) + min);
			titleLabel.Text = title .. "" .. tostring(value);
			callback(value);
		end;
	
		sliderFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true;
			end;
		end);
	
		sliderFrame.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false;
			end;
		end);
	
		sliderFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				slide(input);
			end;
		end);
	
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
				slide(input);
			end;
		end);
	end;
	newSlider("", function(value) if _G.CF == true then game.Workspace.Camera.FieldOfView  = value end end, 40, 120, 80);
end
coroutine.wrap(BFFB_fake_script)()
local function ERFWT_fake_script() -- PizzaDLC.Crosshair.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.Crosshair)

	Zed2 = false
	local Frame = script.Parent.Circle
	script.Parent.MouseButton1Down:connect(function()
	
		if Zed2 == true then 
			Zed2 = false 
		else
			Zed2 = true
		end
		if Zed2 == true then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(180, 5, 255)
			Frame:TweenPosition(UDim2.new(0.566, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1)
		end
		if Zed2 == false then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
			Frame:TweenPosition(UDim2.new(0.036, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1) 
		end
	
		if Zed2 == true then
			
				crosshairlength = 24
	
			local uis = game:GetService('UserInputService')
			local cc = game.Workspace.CurrentCamera;
			local xl = Drawing.new("Line")
			xl.Visible = true;
			local yl = Drawing.new("Line")
			yl.Visible = true;
			xl.Thickness = 1;
			yl.Thickness = 1;
			xl.Color = Color3.fromRGB(179, 0, 255);
			yl.Color = Color3.fromRGB(179, 0, 255);
			xl.From = Vector2.new((cc.ViewportSize.X / 2) + (crosshairlength / 2 + 1), cc.ViewportSize.Y / 2)
			xl.To = Vector2.new(cc.ViewportSize.X / 2 - (crosshairlength / 2), cc.ViewportSize.Y / 2)
			yl.From = Vector2.new(cc.ViewportSize.X / 2, cc.ViewportSize.Y / 2 + (crosshairlength / 2))
			yl.To = Vector2.new(cc.ViewportSize.X / 2, cc.ViewportSize.Y / 2 - (crosshairlength / 2))
			function rainbow(yeet)
				return math.acos(math.cos(yeet * math.pi)) / math.pi
			end
			local count = 0
			local rgb = coroutine.create(function()
				while wait(0.1) do
					pcall(function()
						xl.Color = Color3.fromRGB(179, 0, 255)
						yl.Color = Color3.fromRGB(179, 0, 255)
						count = count + 0.01
					end)
				end
			end)
			coroutine.resume(rgb)
			uis.InputBegan:Connect(function(i)
				if not Zed2 then
					xl:Remove()
					yl:Remove()
				end
			end)
	
		end
	
		if Zed2 == false then 
		
		end
	end)
	
end
coroutine.wrap(ERFWT_fake_script)()
