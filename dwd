end
coroutine.wrap(PCJXE_fake_script)()
local function QUCWPDG_fake_script() -- PizzaDLC.Drag.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.Drag)

	p = script.Parent
	p.Active = true
	p.Draggable = true
	
	
	local UIS = game:GetService("UserInputService")
	local toggle = false
	local plr = game.Players.LocalPlayer
	
	UIS.InputBegan:Connect(function(input, proc)
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Insert then
			toggle = not toggle
			if toggle == true then
				p.Visible = true
				_G.Me.Visible = true
			elseif not toggle then
				p.Visible = false
				_G.Me.Visible = false
			end
		end
	end)
end
coroutine.wrap(QUCWPDG_fake_script)()
local function VMMSA_fake_script() -- PizzaDLC.Drag.Fov Circle 
	local script = Instance.new('LocalScript', PizzaDLC.Drag)

	Mouse = game.Players.LocalPlayer:GetMouse()
	function WTS(obj)
		local screen = workspace.CurrentCamera:WorldToViewportPoint(obj)
		return Vector2.new(screen.x, screen.y)
	end
	
	
	_G.fovcircle = Drawing.new("Circle")
	_G.fovcircle.Visible = false
	_G.fovcircle.Radius = 100
	_G.fovcircle.NumSides = 64
	_G.fovcircle.Color = Color3.fromRGB(184, 2, 255)
	_G.fovcircle.Thickness = 1
	_G.fovcircle.Filled = false
	_G.fovcircle.Transparency = 1
	
	game.RunService.RenderStepped:Connect(function()
		_G.fovcircle.Position = WTS(Mouse.hit.p)
	end)
end
coroutine.wrap(VMMSA_fake_script)()
