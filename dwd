	function newSlider(title, callback, min, max, startPoint)
		local dragging = false;
		local sliderFrame = script.Parent.Parent.SliderFV
		local slidingFrame = script.Parent.Slidefv
		local circle = slidingFrame.Frame
		local titleLabel = script.Parent.stufffv
	
