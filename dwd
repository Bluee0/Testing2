			Frame:TweenPosition(UDim2.new(0.566, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1)
		end
		if Zed == false then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
			Frame:TweenPosition(UDim2.new(0.036, 0,0.5, 0),Enum.EasingDirection.Out,Enum.EasingStyle.Sine,0.1) 
		end
	
		if Zed == true then
			
			script.Parent.Parent.Trc.Visible = true
			script.Parent.Parent.SliderT.Visible = true
			script.Parent.Parent.Trc2.Visible = true
			
	
		end
	
		if Zed == false then 
			script.Parent.Parent.Trc.Visible = false
			script.Parent.Parent.SliderT.Visible = false
			script.Parent.Parent.Trc2.Visible = false
		end
	end)
	
end
coroutine.wrap(BXGJEBJ_fake_script)()
local function QEBXBHE_fake_script() -- PizzaDLC.SliderT.LocalScript 
	local script = Instance.new('LocalScript', PizzaDLC.SliderT)

	function newSlider(title, callback, min, max, startPoint)
		local dragging = false;
		local sliderFrame = script.Parent.Parent.SliderT
		local slidingFrame = script.Parent.SlideT
		local circle = slidingFrame.FrameT
		local titleLabel = script.Parent.stuffT
	
		local function slide(input)
			local pos = UDim2.new(math.clamp((input.Position.X - sliderFrame.AbsolutePosition.X) / sliderFrame.AbsoluteSize.X, 0, 1), 0, 1, 0);
			slidingFrame:TweenSize(pos, Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.0, true);
			local value = math.floor(((pos.X.Scale * max) / max) * (max - min) + min);
			titleLabel.Text = title .. "" .. tostring(value);
			callback(value);
		end;
	
		sliderFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true;
			end;
		end);
	
		sliderFrame.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false;
			end;
		end);
	
		sliderFrame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				slide(input);
			end;
		end);
	
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
				slide(input);
			end;
		end);
	end;
	newSlider("", function(value) ESP.AttachShift = value end, 1, 100, 80);
end
